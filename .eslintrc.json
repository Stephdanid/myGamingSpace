{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parser": 
     "babel-eslint"
    ,
    "rules": {
        "require-jsdoc":"off",
        
        "indent": ["warn", 4],
        
        "no-invalid-this": "off",
        "react/react-in-jsx-scope": "off",
        "react/destructuring-assignment": "off"
     
    }
}
